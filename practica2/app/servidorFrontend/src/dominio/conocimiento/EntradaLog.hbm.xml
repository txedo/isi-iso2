<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dominio.conocimiento">
	<class name="EntradaLog" table="entradalog">
	  	<id name="id" column="id" type="int">
	  		<generator class="native"></generator>
	  	</id>
	  	<property name="usuario" column="usuario" type="string" />
	  	<property name="fecha" column="fecha" type="timestamp" />
	  	<property name="accion" column="accion" type="string" />
	  	<property name="mensaje" column="mensaje" type="string" />
	</class>
</hibernate-mapping>